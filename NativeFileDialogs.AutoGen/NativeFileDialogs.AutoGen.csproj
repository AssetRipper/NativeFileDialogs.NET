<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup Label="Project">
    <TargetFramework>net6.0</TargetFramework>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyTitle>NativeFileDialogs.AutoGen</AssemblyTitle>
    <AssemblyName>NativeFileDialogs.AutoGen</AssemblyName>
  </PropertyGroup>

  <PropertyGroup Label="Nuget">
    <PackageId>NativeFileDialogs.AutoGen</PackageId>
    <Version>0.1.0-alpha</Version>
    <Authors>Speykious</Authors>
    <PackageTags>Native;File;Dialogs;Portal;Gtk</PackageTags>
    <Title>NativeFileDialogs.AutoGen</Title>
    <Description>Auto-generated .NET bindings for NativeFileDialogs (Extended)</Description>
    <Copyright>2022 Speykious</Copyright>
    <PackageProjectUrl>https://github.com/Speykious/NativeFileDialogs.NET</PackageProjectUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup Label="Package References">
    <PackageReference Include="CppSharp" Version="1.0.1" />
  </ItemGroup>

  <ItemGroup Label="Documents">
    <None Include="..\LICENSE" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>
